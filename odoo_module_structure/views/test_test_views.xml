<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_module_structure" model="ir.actions.act_window">
        <field name="name">Test Test</field>
        <field name="res_model">test.test</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="tree_test_test" model="ir.ui.view">
        <field name="name">tree.test.test</field>
        <field name="model">test.test</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="form_test_test" model="ir.ui.view">
        <field name="name">form.test.test</field>
        <field name="model">test.test</field>
        <field name="arch" type="xml">
            <form>
                <field name="name"/>
            </form>
        </field>
    </record>
</odoo>
